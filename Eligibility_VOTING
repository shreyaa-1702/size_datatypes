#include<iostream>
using namespace std;

int main()
{
	int age;

	cout<<"Enter your age: ";
	cin>>age;

	//person is Eligible or not
	//>=13 and <=19
	if(age<=19)
	{
		cout<<"Person is Teenager"<<endl;
	}
	else
	{
		cout<<"Person is Adult"<<endl;
	}


	//condition to check voting eligility
	if(age>=18)
	{
		cout<<"Person is eligible for voting"<<endl;
	}
	else
	{
		cout<<"Person is not eligible for voting"<<endl;
	}

	return 0;
}
